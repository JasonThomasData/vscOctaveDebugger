    "version": "0.2.0",
    "configurations": [
        {
            "type": "OctaveDebugger",
            "request": "launch",
            "name": "Launch Program",
            "program": "${command:AskForProgramName}",
            "octave": "octave-cli",
            "sourceFolder": "${workspaceFolder}",
            "autoTerminate": true
        },
        {
            "type": "OctaveDebugger",
            "request": "launch",
            "name": "TestOctaveDebugger",
            "program": "TestOctaveDebugger",
            "octave": "octave-cli",
            "sourceFolder": "${workspaceFolder}"
        },
        {
            "type": "OctaveDebugger",
            "request": "launch",
            "name": "hello",
            "program": "hello",
            "octave": "octave-cli",
            "sourceFolder": "${workspaceFolder}"
        },
        {
            "type": "OctaveDebugger",
            "request": "launch",
            "name": "Debug Extension",
            "program": "TestOctaveDebugger",
            "octave": "octave-cli",
            "sourceFolder": "${workspaceFolder}",
            "debugServer": 4711
        },
        {
            "type": "OctaveDebugger",
            "request": "launch",
            "name": "Debug Octave fieldnames",
            "program": "TestOctaveDebugger",
            "splitFieldnamesOctaveStyle": true,
            "octave": "octave-cli",
            "sourceFolder": "${workspaceFolder}",
            "debugServer": 4711
        }
    ]
}